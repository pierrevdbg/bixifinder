    <%= form.number_field :nbBikes, id: :bixi_nbBikes %>
  </div>

  <div class="field">
    <%= form.label :nbEmptyDocks %>
    <%= form.number_field :nbEmptyDocks, id: :bixi_nbEmptyDocks %>
  </div>

  <div class="field">
    <%= form.label :lastUpdateTime %>
    <%= form.date_select :lastUpdateTime, id: :bixi_lastUpdateTime %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
